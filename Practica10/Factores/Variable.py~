class Variable:

    def __init__(self,nombre,valores = []):
        self.nombre = nombre
        self.valores = valores

    def get_nombre(self):
        return self.nombre

    def set_nombre(self,nombre):
        self.nombre = nombre

    def get_valores(self):
        return self.valores

    def set_valores(self,valores):
        self.valores = valores

    
